{
  "manifestVersion": 1,
  "id": "Vsts-Release-Terraform",
  "name": "Terraform",
  "version": "1.0.3",
  "publisher": "tylerevert",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Build extension for running Terraform commands.",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "terraform",
    "release",
    "build",
    "azure",
    "azurerm",
    "aws",
    "iac",
    "google",
    "cloud",
    "infrastucture as code",
    "Hashicorp",
    "deploy",
    "template"
  ],
  "screenshots": [
    {
      "path": "images/screenshot.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/tevert/Vsts-Release-Terraform"
    },
    "support": {
      "uri": "https://github.com/tevert/Vsts-Release-Terraform"
    }
  },
  "branding": {
    "color": "rgb(36, 43, 50)",
    "theme": "dark"
  },
  "icons": {
    "default": "images/terraform.png"
  },
  "files": [
    {
      "path": "extension"
    }
  ],

  "contributions": [
    {
      "id": "Vsts-Release-Terraform",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "extension"
      }
    }
  ]
}
